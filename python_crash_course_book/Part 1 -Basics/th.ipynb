     "   pygame.display.set_caption('Untitled')\n",
     "   win.fill((255,255,255))\n",
     "\n",
     "   #CREATION OF OBJECTS\n",
     "   grid = pixelArt(win, int(wid), int(heigh), cols, rows, showGrid)\n",
